		// Import declaration
import java.util.*;
		
public class Project1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		//Import Declaration 
		Scanner console = new Scanner(System.in);
		// TODO Auto-generated method stub
		/* Define the sum of squares */
		double sumofsquares = console.nextDouble();
		
		/* Define the number of trials */
		int trials = console.nextInt();
		
		/* Display phrase */
		System.out.println("The Standard Deviation is:");
		
		/*Standard deviation formula */
		System.out.println(Math.sqrt( sumofsquares ) / ( trials - 1 ));
		
		
	}
	public static void random(double sumofsquares, int trials) {
		/* This is an extra method to demonstrate that I know how to make multiple methods */
		System.out.println( "sumofsquares + trials" );
	}

}
